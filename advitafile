pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git url: 'https://github.com/yogeshbhoiraws21/mygitproject.git', branch: 'main'
            }
        }

        stage('Deploy') {
            steps {
                sshagent(['sshkey']) {
                    sh 'scp -o StrictHostKeyChecking=no index.html ubuntu@100.26.52.126:/var/www/html/'
                }
            }
        }
    }
}
